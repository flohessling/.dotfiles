/* The default configuration incase none is specified */
configuration {
	width: 500;
	show-icons: true;
	terminal: "gnome-terminal";
	auto-select: false;
	theme: "nord";
	drun-display-format: "{icon} {name}";
	font: "JetBrains Mono 10";
	modi: "window,run,drun";
	show-icons: true;
}

@import "nord"
